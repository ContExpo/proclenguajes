//factor :	<tNOT> factor
//		|	<tPTOP> expresion <tPTCL>
//		|	<tINT2CHAR> <tPTOP> expresion <tPTCL>
//		|	<tCHAR2INT> <tPTOP> expresion <tPTCL>
//		|	LOOKAHEAD(2)  <tID> <tPTOP> lista_exps <tPTCL> Esta es una llamada a funcion
//		|	LOOKAHEAD(2)  <tID> <tSQOP> expresion <tSQCL>  componente array
//		|	<tID>
//		|	<tNUM>
//		|	<tCHAR>
//		|	<tSTRING>
//		|	<tTRUE>
//		|	<tFALSE>
Attributes factor() :
{
	Attributes a = new Attributes();
	Attributes temp;
	Token t;
	Symbol s;
}
{
	try {
		<tNOT>
		a = factor(){ 
			if (a.type != Symbol.Types.BOOL) {
				es.deteccion("El tipo de la expresion tiene que ser booleano", token);
			}
			a.referenciable = false;
		}
	|	<tPTOP>
		a = expresion()
		<tPTCL>
	|	<tITOA>
		<tPTOP>
		//A int to ascii tienes que pasarle un int
		temp = expresion()
		{
			if (temp.type != Symbol.Types.INT) {
				es.deteccion("El tipo de la expresion tiene que ser intero", token);
			}
			a.type = Symbol.Types.CHAR;
			a.referenciable = false;
		}
		<tPTCL>
	|	<tATOI>
		<tPTOP>
		//A ascii to int tienes que pasarle un char
		temp = expresion()
		{
			if (temp.type != Symbol.Types.CHAR) {
				es.deteccion("El tipo de la expresion tiene que ser character", token);
			}
			a.type = Symbol.Types.INT;
			a.referenciable = false;
		}
		<tPTCL>
	|	LOOKAHEAD(2) 
		a = inst_invoc_func()
	|	LOOKAHEAD(2) 
		t = <tID>	//Esto es id de array
		{
			s = st.getSymbol(t.image);
			if (!st.containsSymbol(t.image)) {
				es.deteccion(new SymbolNotFoundException(), t);
			} else {
				if (s.type != Symbol.Types.ARRAY) {
					es.deteccion(t.image + "no es identificador de array", t);
				}
				a.type = s.type;
				a.referenciable = true;
				a.name = t.image;
				a.simbolo = st.getSymbol(t.image);
			}
		}
		<tSQOP>
		temp = expresion() { 
			if (a.type != Symbol.Types.INT)
				es.deteccion("El indice de elemento de array tiene que ser intero", t);
		}
		<tSQCL>
	|	t = <tID>
		{
			if (!st.containsSymbol(t.image)) {
				es.deteccion(new SymbolNotFoundException(), t);
			} else {
				s = st.getSymbol(t.image);
				if (s.type == Symbol.Types.ARRAY || s.type == Symbol.Types.FUNCTION || s.type == Symbol.Types.PROCEDURE) {
					es.deteccion(t.image + "no puede ser utilizado como variable simple", t);
				}
				a.type = s.type;
				a.referenciable = true;
				a.name = t.image;
				a.simbolo = s;
			}
		}
	|	<tNUM>
		{
			a.type = Symbol.Types.INT;
			a.referenciable = false;
		}
	|	<tCHAR>
		{
			a.type = Symbol.Types.CHAR;
			a.referenciable = false;
		}
	|	<tSTRING>
		{
			a.type = Symbol.Types.STRING;
			a.referenciable = false;
		}
	|	<tTRUE>
		{
			a.type = Symbol.Types.BOOL;
			a.referenciable = false;
		}
	|	<tFALSE>
		{
			a.type = Symbol.Types.BOOL;
			a.referenciable = false;
		}
	} catch (ParseException e) {
		Set<Integer> conjSinc = infoParseException(e);
		//añadir tokens al conj de sincronización
		conjSinc.add(tEND);
		conjSinc.add(tSCOL);
		conjSinc.add(tPTCL);
		conjSinc.add(tCOMMA);
		conjSinc.add(tSQCL);
		recuperacionPanico(e.currentToken.next, conjSinc);
	}
	{
		return a;
	}
}

