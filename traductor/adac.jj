//expresion_simple : ( <tADD> | <tSUB> )? termino ( operador_aditivo termino )*
Attributes expresion_simple() :
{
	Attributes a = new Attributes();
	Attributes a2 = null;
}
{
	try {
		(
			<tADD> 	{
				a2 = new Attributes();
			}
		|	<tSUB> {
				a2 = new Attributes();
			}
		)?
		a = termino()
		{
			if (a2!=null){
				if (a.type != Symbol.Types.INT)
					es.deteccion("La variable despues de +/- tiene que ser intera", token);
			}
			a.referenciable = false;
		}
		(
			a2 = operador_aditivo() {
				if (a2.type != a.type)
					es.deteccion("Los tipos del operando y el termino no coinciden", token);
				a.referenciable = false;
			}
			a2 = termino() {
				if (a2.type != a.type)
					es.deteccion("Los tipos de los terminos de la suma no coinciden", token);
				a.referenciable = false;
			}
		)*
	} catch (ParseException e) {
		Set<Integer> conjSinc = infoParseException(e);
		//añadir tokens al conj de sincronización
		conjSinc.add(tEND);
		conjSinc.add(tSCOL);
		conjSinc.add(tPTCL);
		conjSinc.add(tCOMMA);
		recuperacionPanico(e.currentToken.next, conjSinc);
	}
	{
		return a;
	}
}

