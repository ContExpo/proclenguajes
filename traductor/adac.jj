{
	boolean hayReturn1 = false;
	boolean hayReturn2 = false;
	boolean aux = false;
	Attributes attr;
}
{
	try {
		<tIF>
		attr = expresion() {
			if (attr.type != Symbol.Types.BOOL)
				es.deteccion("El tipo de la expresion tiene que ser booleano", token);
		}
		<tTHEN>
		(
