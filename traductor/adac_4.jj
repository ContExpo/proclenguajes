//termino : factor ( operador_multiplicativo factor )*
Attributes termino() :
{
	Attributes a = null;
	Attributes a2 = null;
}
{
	try {
		a = factor()
		(
			a2 = operador_multiplicativo() {
				if (a2.type != a.type)
					es.deteccion("Los tipos del operando y el operator no coinciden", token);
			}
			a2 = factor() {
				if (a2.type != a.type)
					es.deteccion("Los tipos de los operandos no coinciden: " + a.type + " vs " + a2.type, token);
